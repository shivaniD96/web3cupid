# ============================================================
# CryptoCupid Environment Configuration
# ============================================================
# Copy this file to .env and fill in your values
# ⚠️ NEVER commit .env with real keys!
# ============================================================

# ============ REQUIRED FOR DEPLOYMENT ============

# Your deployer wallet private key (without 0x prefix)
PRIVATE_KEY=your_private_key_here

# ============ NETWORK RPCs ============

# Arbitrum Sepolia (primary testnet)
ARBITRUM_SEPOLIA_RPC=https://sepolia-rollup.arbitrum.io/rpc

# Ethereum Sepolia
ETHEREUM_SEPOLIA_RPC=https://rpc.sepolia.org

# Mainnet (for production - fill when ready)
ARBITRUM_RPC=https://arb1.arbitrum.io/rpc
ETHEREUM_RPC=https://eth.llamarpc.com

# ============ CONTRACT VERIFICATION ============

ETHERSCAN_API_KEY=your_etherscan_api_key
ARBISCAN_API_KEY=your_arbiscan_api_key

# ============ OPTIONAL ============

# Gas reporting
REPORT_GAS=false
COINMARKETCAP_API_KEY=

# ============ FRONTEND ============

# Contract addresses (auto-generated after deployment)
NEXT_PUBLIC_CRYPTO_CUPID_ADDRESS=
NEXT_PUBLIC_CUPID_TOKEN_ADDRESS=
NEXT_PUBLIC_CHAIN_ID=421614

# WalletConnect project ID (get from cloud.walletconnect.com)
NEXT_PUBLIC_WALLETCONNECT_PROJECT_ID=

# ============ VERIFICATION ORACLE ============
# Address that can verify users (Worldcoin/Gitcoin integration)
# Leave empty to use deployer address for testing
VERIFICATION_ORACLE_ADDRESS=
