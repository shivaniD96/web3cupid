{
  "name": "crypto-cupid",
  "version": "1.0.0",
  "description": "Privacy-preserving dating app for crypto folks using Fhenix FHE",
  "main": "index.js",
  "scripts": {
    "compile": "hardhat compile",
    "test": "hardhat test",
    "deploy:local": "hardhat run scripts/deploy.js --network localfhenix",
    "deploy:arb-sepolia": "hardhat run scripts/deploy.js --network arbitrumSepolia",
    "deploy:eth-sepolia": "hardhat run scripts/deploy.js --network ethereumSepolia",
    "verify": "hardhat verify",
    "clean": "hardhat clean",
    "node": "hardhat node",
    "generate-abi": "hardhat compile && node scripts/generate-abi.js"
  },
  "dependencies": {
    "@fhenixprotocol/cofhe-contracts": "0.0.13",
    "cofhejs": "^0.3.1",
    "@openzeppelin/contracts": "^5.2.0",
    "ethers": "^6.13.2"
  },
  "devDependencies": {
    "@nomicfoundation/hardhat-toolbox": "^5.0.0",
    "@nomicfoundation/hardhat-verify": "^2.0.0",
    "cofhe-hardhat-plugin": "^0.3.1",
    "@fhenixprotocol/cofhe-mock-contracts": "^0.3.1",
    "hardhat": "^2.26.1",
    "dotenv": "^16.4.5",
    "chai": "^4.5.0"
  },
  "keywords": ["fhenix", "fhe", "dating", "crypto", "privacy", "web3"],
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0"
  }
}
